/* Header related styles from devzone navigation.css */ 

body {
	position: relative;
}

#header {
	width: 960px;
	margin: 0 auto;
	padding: 0;
	overflow: hidden;
	line-height: 28px;
	font-weight: bold;
}
    #header form {
    	display: inline;
    }
    #footer fieldset, #header fieldset {
        display: inline;
        line-height: 1em;
        vertical-align: middle;
    }
	#header #searchSet {
		display: inline;
	}
	
#header.contentpage {
	background: url("http://replay.waybackmachine.org/20110412182553/http://download.skype.com/share/blogskin/devzone/icon_puzzle_header.png") top left no-repeat;
}

#header.contentpage #logoSkype {
	margin-left: 130px;
}

#header.contentpage .tabs {
	margin-left: 130px;
}

#skypeLogo {
	float: left;
	width: 30%;
	overflow: auto;
	text-decoration: none;
}
	#skypeLogo img {
		margin: 0 15px 0 20px;
		display: block;
		float: left;
		display: inline;
	}
	#skypeLogo span {
		display: block;
		float: left;
		color: #7FD7F7;
	}
#headerNavigation {
	float: right;
	width: 70%;
	text-align: right;
	color: #AFB4B4;
}
	#headerNavigation a {
		margin: 0 4px;
		color: #00AFF0;
		text-decoration: none;
	}
	#headerNavigation a:hover {
	    text-decoration: underline;
	}
	#headerNavigation input {
		margin: 0 20px 0 6px;
	}
    #headerNavigation span {
        margin: 0;
        color: #636363;
    }



/* TABS */

div.tabs {
	margin: 0;
	padding: 0;
/*	width: 100%;*/
}
div.tabs span {
	font-size: 0.9em;
}
div.tabs ul {
	padding: 0 !important;
	margin: 0 0 0 20px !important;
}
div.tabs ul li {
	float: left;
	margin-right: 5px;
}

div.tabs ul li a {
	float: left;
	display: block;
	list-style: none;
	line-height: 28px;
	padding: 0px 10px 0px 0px;
	background: transparent url("http://replay.waybackmachine.org/20110412182553/http://download.skype.com/share/blogskin/devzone/tab_blue.png") top right no-repeat;
	text-decoration: none;
	cursor: pointer;
	color: #ffffff;
}
div.tabs ul li a strong {
	float: left;
	display: block;
	color: #ffffff;
	text-decoration: none;
	background: transparent url("http://replay.waybackmachine.org/20110412182553/http://download.skype.com/share/blogskin/devzone/tab_blue.png") top left no-repeat;
	padding: 0px 0px 0px 10px;
	font: bold 14px/28px Arial, Helvetica, sans-serif;
	cursor: pointer;
}

div.tabs ul li.selected a{
	background: url("http://replay.waybackmachine.org/20110412182553/http://download.skype.com/share/blogskin/devzone/tab_white.png") top right no-repeat;
	color: #000000;
}
div.tabs ul li.selected a strong {
	background: url("http://replay.waybackmachine.org/20110412182553/http://download.skype.com/share/blogskin/devzone/tab_white.png") top left no-repeat;
	color: #000000;
}

div.tabs ul li.selected a:hover {
	background: url("http://replay.waybackmachine.org/20110412182553/http://download.skype.com/share/blogskin/devzone/tab_white.png") top right no-repeat;
	color: #000000;
}
div.tabs ul li.selected a:hover strong {
	background: url("http://replay.waybackmachine.org/20110412182553/http://download.skype.com/share/blogskin/devzone/tab_white.png") top left no-repeat;
	color: #000000;
}

div.tabs ul li a:hover {
	background: transparent url("http://replay.waybackmachine.org/20110412182553/http://download.skype.com/share/blogskin/devzone/tab_lightblue.png") top right no-repeat;
}
div.tabs ul li a:hover strong {
	background: transparent url("http://replay.waybackmachine.org/20110412182553/http://download.skype.com/share/blogskin/devzone/tab_lightblue.png") top left no-repeat;

}


/* Header */

#headerWrapper {
	background: #00aff0;
	width: 100%;
}

#header {
	width: 960px;
	margin: 0 auto;
}

#logoSkype {
	margin: 15px 0 10px 0px;
	padding-left: 20px;
	float: left;
}

#logoSkype #logo {
	padding-left: 130px;
}

#searchWrapper {
	float: right;
	margin: 10px 0 5px 0;
}

#searchWrapper a {
	text-decoration: none;
}

#searchBox {
	margin: 0 20px 0 0;
	padding: 0;
	float: left;
	line-height: 24px;
}

#userPreferences {
	float: left;
	font-size: 11px;
	font-family: Helvetica, Arial, sans-serif;
	color: #ffffff;
	line-height: 28px;
	margin: 0 20px 0 0;
}


#userPreferences a#userhome {
	background: url(http://replay.waybackmachine.org/20110412182553/http://download.skype.com/share/blogskin/devzone/username.png) 0px 4px no-repeat;
	padding: 0 0 0 20px;
	margin: 0 20px 0 0;
	font-weight: bold;
}

#userPreferences a {
	color: #fff;
	line-height: 28px;
